    "start-test": "ENV=test PORT=1234 node ./node_modules/nodemon/bin/nodemon ./ localhost 1234",
    "test-unit": "node ./node_modules/mocha/bin/mocha ./specs/ --recursive",
    "test-feature": "node ./node_modules/cucumber/bin/cucumber.js --strict -r ./features",
